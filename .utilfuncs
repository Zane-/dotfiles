function ah () {
	date=`date +%m-%d-%Y`
	name="Zane Bilous"
	id="zb7aa"
	for i do
		if ! grep -q "Computing ID:" $i
		then
		header="/**\n * Name: $name\n * Computing ID: $id\n * Date: $date\n * Filename: $i\n*/" 
			echo -e "$header\n$(cat $i)" > $i
		fi
	done
}

